{
  "name": "@ask-utils/amazon-pay",
  "version": "1.0.1",
  "description": "Utility libs for the Alexa skill to integrate the Amazon Pay APIs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "doc": "typedoc --out docs libs/",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ask-utils/ask-utils-for-amazon-pay.git"
  },
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ask-utils/ask-utils-for-amazon-pay/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ask-utils/ask-utils-for-amazon-pay#readme",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.17",
    "ask-sdk-model": "^1.10.1",
    "jest": "^24.8.0",
    "ts-jest": "^23.10.5",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*/*.+(ts|tsx|js)"
    ]
  },
  "directories": {
    "doc": "docs"
  }
}
